  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "declaration": false,
    "noImplicitAny": false,
    "outDir": "out-tsc"
  },
  "exclude": [
    "node_modules",
    "asyncAwait",
    "plugins.ts",
    "testresults"
  ]
}